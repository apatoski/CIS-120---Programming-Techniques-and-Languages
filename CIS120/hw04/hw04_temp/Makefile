# DO NOT EDIT THIS FILE

SUBMIT      := imp.ml queueTest.ml linkedQueue.ml simpleQueue.ml dequeTest.ml deque.ml graph.ml

DEPENDS := \
	assert.ml \
	assert.mli \
	queueInterface.ml \
	queueInterface.mli \
	imp.ml \
	imp.mli \
	queueTest.ml \
	linkedQueue.ml \
	linkedQueue.mli \
	simpleQueue.ml \
	simpleQueue.mli \
	dequeTest.ml \
	deque.ml \
	deque.mli \
	graph.ml \
	graph.mli 


HWNAME := hw04
ts := $(shell /bin/date "+%Y-%m-%d-%H:%M:%S")

ZIPNAME := $(HWNAME)-submit($(ts)).zip

.PHONY: all  clean zip

all:	imp.native queueTest.native linkedQueue.native simpleQueue.native dequeTest.native deque.native graph.native

%.native: $(DEPENDS)
	ocamlbuild -lib graphics $@

run% : %.native $(DEPENDS)
	./$<


zip: $(SUBMIT)
	zip '$(ZIPNAME)' $(SUBMIT)

clean:
	ocamlbuild -clean
	rm -rf *.zip
